  --% AVAILABILITY public
      ::= { cyOnSiteTrapObject 29 }

cyOnSiteTrapObjectFanNumber OBJECT-TYPE
      SYNTAX INTEGER
      MAX_ACCESS not-accessible
      STATUS current
      DESCRIPTION
      "This object is sent in a trap to identify which fan the trap
       condition occured on."
  --% AVAILABILITY public
      ::= { cyOnSiteTrapObject 30 }

cyOnSiteTrapObjectFanSpeed OBJECT-TYPE
      SYNTAX INTEGER
      MAX_ACCESS not-accessible
      STATUS current
      DESCRIPTION
