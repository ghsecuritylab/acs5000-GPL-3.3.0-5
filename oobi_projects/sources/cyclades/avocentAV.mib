END

AVOCENT-AUTOVIEW-SYS-MIB DEFINITIONS ::= BEGIN

IMPORTS
	avManagement
		FROM AVOCENT-AUTOVIEW-MIB
	OBJECT-TYPE, MODULE-IDENTITY, OBJECT-IDENTITY, Integer32
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC;

avSystem MODULE-IDENTITY
	LAST-UPDATED	"200704260000Z"
	ORGANIZATION	"Avocent Corporation"
	CONTACT-INFO	"postal: Avocent Corporation
			3541 Gateway Boulevard
			Fremont, CA 94538, USA
			e-mail: Technical Support
			techsupport@avocent.com"
	DESCRIPTION	"This module defines AutoView Family System Information objects"
	REVISION	"200704260000Z"
	DESCRIPTION	"First Draft"
::= { avManagement 1 }

avPname OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "The product's name - AVCS40xx (xx - the number of the serial ports)"
::= { avSystem 1 }

avVersion OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "The firmware's version"
::= {avSystem 2}

avPower OBJECT-IDENTITY
	STATUS current
	DESCRIPTION "It gives information about Power Supply"
::= { avSystem 3 }

avPwNum OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "Number of the power supplies"
::= { avPower 1 }

avPw1 OBJECT-TYPE
	SYNTAX INTEGER {
		noinstalled(0),
		powerON(1),
		powerOFF(2)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "State of the first power supply"
::= { avPower 2 }

avPw2 OBJECT-TYPE
	SYNTAX INTEGER {
		noinstalled(0),
		powerON(1),
		powerOFF(2)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "State of the second power supply"
::= { avPower 3 }

avPcmcia OBJECT-IDENTITY
	STATUS current
	DESCRIPTION "PCMCIA card information"
::= {avSystem 4}

avNPcmcia OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "Number of the cards PCMCIA available"
::= { avPcmcia 1 }

avCardIdentTable OBJECT-TYPE
	SYNTAX SEQUENCE OF AvCardIdentEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION "Card identification"
::= { avPcmcia 2 }

avCardIdentEntry OBJECT-TYPE
	SYNTAX AvCardIdentEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION "One particular association between PCMCIA socket and card identification"
	INDEX { avCardIdentIndex }
::= { avCardIdentTable 1 }

AvCardIdentEntry ::= SEQUENCE {
	avCardIdentIndex Integer32,
	avCardIdentProd DisplayString,
	avCardIdentMan DisplayString ,
	avCardIdentFunc DisplayString,
	avCardIdentPCI DisplayString
}

avCardIdentIndex OBJECT-TYPE
	SYNTAX Integer32(0..2)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "number to identifier the socket Card"
::= { avCardIdentEntry 1 }

avCardIdentProd OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "Product identification strings"
::= { avCardIdentEntry 2 }

avCardIdentMan OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "Manufacturer ID codes"
::= { avCardIdentEntry 3 }

avCardIdentFunc OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "Function ID code"
::= { avCardIdentEntry 4 }

avCardIdentPCI OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "PCI ID"
::= { avCardIdentEntry 5 }

avCardConfTable OBJECT-TYPE
	SYNTAX SEQUENCE OF AvCardConfEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION "This table has the socket configuration "
::= { avPcmcia 3 }

avCardConfEntry OBJECT-TYPE
	SYNTAX AvCardConfEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION "One particular association between PCMCIA socket and its configuration"
	INDEX { avCardConfIndex }
::= { avCardConfTable 1 }

AvCardConfEntry ::= SEQUENCE {
	avCardConfIndex Integer32,
	avCardConfPower DisplayString,
	avCardConfType DisplayString ,
	avCardConfInter DisplayString,
	avCardConfFunc DisplayString,
	avCardConfCardv DisplayString,
	avCardConfPort1 DisplayString,
	avCardConfPort2 DisplayString
}

avCardConfIndex OBJECT-TYPE
	SYNTAX Integer32(0..2)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "number to identifier the socket PCMCIA"
::= { avCardConfEntry 1 }

avCardConfPower OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "Power Settings"
::= { avCardConfEntry 2 }

avCardConfType OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "Interface Type"
::= { avCardConfEntry 3 }

avCardConfInter OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "Interrupt number"
::= { avCardConfEntry 4 }

avCardConfFunc OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "function"
::= { avCardConfEntry 5 }

avCardConfCardv OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "Card values"
::= { avCardConfEntry 6 }

avCardConfPort1 OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "I/O Port 1"
::= { avCardConfEntry 7 }

avCardConfPort2 OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "I/O Port 2"
::= { avCardConfEntry 8 }

avCardStatusTable OBJECT-TYPE
	SYNTAX SEQUENCE OF AvCardStatusEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION "This table has the current socket status flags "
::= { avPcmcia 4 }

avCardStatusEntry OBJECT-TYPE
	SYNTAX AvCardStatusEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION "One particular association between PCMCIA socket and its status"
	INDEX { avCardStatusIndex }
::= { avCardStatusTable 1 }

AvCardStatusEntry ::= SEQUENCE {
	avCardStatusIndex Integer32,
	avCardStatusCard DisplayString,
	avCardStatusFunc DisplayString
}

avCardStatusIndex OBJECT-TYPE
	SYNTAX Integer32(0..2)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "number to identifier the socket PCMCIA"
::= { avCardStatusEntry 1 }

avCardStatusCard OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "Current Card State"
::= { avCardStatusEntry 2 }

avCardStatusFunc OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "Current Function"
::= { avCardStatusEntry 3 }

avFlashSize OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "Total amount of flash memory in Mbytes"
::= {avSystem 5}

avRAMSize OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "Total amount of RAM in Mbytes"
::= {avSystem 6}

avCPUfreq OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "CPU clock in MHz"
::= {avSystem 7}

avDevId OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "The product's identifier"
::= { avSystem 8 }

END

AVOCENT-AUTOVIEW-CONF-MIB DEFINITIONS ::= BEGIN

IMPORTS
	avManagement
			FROM AVOCENT-AUTOVIEW-MIB
	InterfaceIndexOrZero
			FROM IF-MIB
	MODULE-IDENTITY, OBJECT-TYPE, OBJECT-IDENTITY,
	IpAddress, Integer32
			FROM SNMPv2-SMI
	DisplayString
			FROM SNMPv2-TC;

avConfiguration    MODULE-IDENTITY
	LAST-UPDATED	"200704260000Z"
	ORGANIZATION	"Avocent Corporation"
	CONTACT-INFO	"postal: Avocent Corporation
			3541 Gateway Boulevard
			Fremont, CA 94538, USA
			e-mail: Technical Support
			techsupport@avocent.com"
	DESCRIPTION	"This module defines AutoView Family Configuration objects"
	REVISION	"200704260000Z"
	DESCRIPTION	"First Draft"
::= { avManagement 2 }

-- Current Avocent AutoView Family Configuration core mib table entries :
--	avHostName			OBJECT-TYPE	::= { avConfiguration 1 }
--	avConsoleBanner		OBJECT-TYPE	::= { avConfiguration 2 }
--	avMotd				OBJECT-TYPE	::= { avConfiguration 3 }
--	avEthItf			OBJECT-IDENTITY	::= { avConfiguration 4 }
--	avNameService		OBJECT-IDENTITY	::= { avConfiguration 5 }  
--	avSerialPortConf	OBJECT-IDENTITY	::= { avConfiguration 6 }

avHostName	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (1..40))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The Host Name "
::= { avConfiguration 1 }                              

avConsoleBanner	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (1..255))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The console banner "
::= { avConfiguration 2 }                              

avMotd	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..255))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The motd message "
::= { avConfiguration 3 }                              

avEthItf	OBJECT-IDENTITY
	STATUS     	current
	DESCRIPTION "The configuration of the Ethernet Interface"   
::= { avConfiguration 4 }

avEthDhcpc	OBJECT-TYPE
	SYNTAX	INTEGER	{
			inactive(0),	-- DHCP is inactive
			active(1),	-- DHCP is active
			restore(2)	-- DHCP is active and AVCS saves the last parameters 
					-- assigned by the server
			}
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The AVCS can request all of its ethernet parameters 
				 to a DHCP server"
::= { avEthItf 1 }

avEthIPaddr	OBJECT-TYPE
	SYNTAX		IpAddress
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"This is the IP address of the Ethernet Interface"
::= { avEthItf 2 }

avEthIPmask		OBJECT-TYPE
	SYNTAX		IpAddress
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The mask fot the Ethernet network"
::= { avEthItf 3 }

avEthMTU		OBJECT-TYPE
	SYNTAX		Integer32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The Maximum Transmission Unit size, wich determines 
			 whether or not packets should be broken up"
::= { avEthItf 4 }

avEthIPaddr2	OBJECT-TYPE
	SYNTAX		IpAddress
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The Ethernet secondary IP address"
::= { avEthItf 5 }

avEthIPmask2	OBJECT-TYPE
	SYNTAX		IpAddress
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The Ethernet secondary IP mask"
::= { avEthItf 6 }

avNameService	OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION	"The Name Service configuration"
::= { avConfiguration 5 }  

avResolverOrder	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..15))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"It specifies how host lookups are to be performed.
			 Valid values : empty, 'hosts', 'bind', 'hosts, bind', 
			 'bind, hosts' "
::= { avNameService 1 }
                   
avMultipleIP	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..3))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Valid values are ON or OFF. If set to ON, the resolve 
			 library will return all valid addresses for a host 
			 that appears in the /etc/hosts file, instead of only 
			 the first.  "
::= { avNameService 2 }

avDNSserv		OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION	"The DNS server configuration"
::= { avNameService 3 }

avDNSpriserv	OBJECT-TYPE
	SYNTAX		IpAddress
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Internet Address (in dot notation) of primary name 
			 server that the resolver should query."
::= { avDNSserv 1 }
 
avDNSsecserv	OBJECT-TYPE
	SYNTAX		IpAddress
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Internet Address (in dot notation) of secondary name 
			 server that the resolver should query."
::= { avDNSserv 2 }

avDNSdomain		OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (1..40))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Local domain name ."
::= { avDNSserv 3 }

avSerialPortConf	OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION	"The PortSlave configuration - pslave.conf file. 
			 It configures the all serial ports."
::= { avConfiguration 6 }  

avSerialGlobal		OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION	"These parameters have the prefix conf. in 
			 the pslave.conf file"
::= { avSerialPortConf 1 }

avSerialInclude	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..40))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"File used in centralized management."
::= { avSerialGlobal 1 }

avSerialNFS		OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..50))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Remote Network File System where data buffering will be 
			 written instead of the default directory '/var/run/DB'."
::= { avSerialGlobal 2 }

avSerialLockDir		OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..50))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The lock directory for the AVCS."
::= { avSerialGlobal 3 }

avSerialRlogin		OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..50))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Location of the rlogin binary that accepts the -i flag."
::= { avSerialGlobal 4 }

avSerialPppd	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..50))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Location of the pppd binary."
::= { avSerialGlobal 5 }

avSerialTelnet	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..50))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION "Location of the telnet application"
::= { avSerialGlobal 6 }

avSerialSsh		OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..50))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Location of the ssh application"
::= { avSerialGlobal 7 }

avSerialLocalLogins	OBJECT-TYPE
	SYNTAX		INTEGER {
				off(0),           
				on(1)   -- possible to login to the AVCS directly by placing a !
			}
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION 	"This parameter is only necessary when authentication is 
			 being performed for a port.
			 This is useful if the Radius authentication server is down."
::= { avSerialGlobal 8 }

avSerialFacility	OBJECT-TYPE
	SYNTAX		Integer32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"This value (0-7) is the Local facility sent to the syslog daemon."
::= { avSerialGlobal 9 }

avSerialDBFacility	OBJECT-TYPE
	SYNTAX		Integer32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"This value (0-7) is the Local facility sent to the syslog 
			 daemon with the data when ."
::= { avSerialGlobal 10 }

avSerialGroupTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF AvGroupEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"This associates group name and its users"
::= { avSerialGlobal 11 }

avgroupEntry	OBJECT-TYPE
	SYNTAX		AvGroupEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"One particular association between group name and its users"
	INDEX		{ avGroupIndex }
::= { avSerialGroupTable 1 }

AvGroupEntry ::= SEQUENCE  {
	avGroupIndex	InterfaceIndexOrZero,
	avGroupName	DisplayString,
	avGroupUsers	DisplayString
}

avGroupIndex	OBJECT-TYPE
	SYNTAX		InterfaceIndexOrZero	
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"number to identifier the group"
::= { avgroupEntry  1 }                              

avGroupName	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..20))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The Group Name "
::= { avgroupEntry  2 }                              

avGroupUsers	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..80))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The users names "
::= { avgroupEntry  3 }                              

avSerialSpec	OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION	"These parameters have the prefix all. or sxx. in the pslave.conf file"
::= { avSerialPortConf 2 }

avSerialPortTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF AvSPortEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION 	"This is the specific configuration to serial port"
::= { avSerialSpec 1 }

avsportEntry	OBJECT-TYPE
	SYNTAX		AvSPortEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	"One particular serial port configuration"
	INDEX		{ avSPortNumber }
::= { avSerialPortTable 1 }

AvSPortEntry ::= SEQUENCE  {
	avSPortNumber		InterfaceIndexOrZero,	-- 0 == all.  X == ttyX sX.
	avSPortTty		DisplayString,		-- associate device
	avSPortName		DisplayString,		-- alias name
	avSPortSpeed		INTEGER,		-- physical port configuration
	avSPortDataSize		Integer32,
	avSPortStopBits		Integer32,
	avSPortParity		DisplayString,
	avSPortFlowCtrl		DisplayString,
	avSPortDTRdelay		Integer32,
	avSPortDCDCtrl		INTEGER,
	avSPortLogUtmp		INTEGER,		-- log into UTMP
	avSPortLogWtmp		INTEGER,		-- log into WTMP
	avSPortLogform		DisplayString,
	avSPortAuthtype		DisplayString,		-- authentication type
	avSPortAuthSrv1		IpAddress,	-- IP address of the primary authentication server
	avSPortAccSrv1		IpAddress,	-- IP address of the primarty accounting server
	avSPortAuthTmo		Integer32,	-- time out per authentication query
	avSPortAuthRetr		Integer32,	-- authentication number of retries
	avSPortAuthSrv2		IpAddress,	-- IP address of the secondary authentication server
	avSPortAccSrv2		IpAddress,	-- IP address of the secondary accounting server
	avSPortAuthSecret	DisplayString,	-- secret used in authentication
	avSPortAuthRadP		INTEGER,	-- accept RADIUS' user without password
	avSPortAuthAcc		DisplayString,	-- access restrition on Users
	avSPortProtocol		DisplayString,	-- Profile
	avSPortRemoteIP		DisplayString,	-- remote IP address
	avSPortSocketPort	DisplayString,	-- number of socket port
	avSPortRemHost		IpAddress,	-- IP address of the remote Host
	avSPortBanner		DisplayString,	-- Banner
	avSPortPrompt		DisplayString,	-- Login prompt
	avSPortTermType		DisplayString,	-- terminal type
	avSPortAutomUsr		DisplayString,	-- automatic user
	avSPortNetMask		IpAddress,	-- net mask
	avSPortPppMtu		Integer32,
	avSPortPppMru		Integer32,
	avSPortPppOptions	DisplayString,
	avSPortPppFoption	DisplayString,
	avSPortModemChat	DisplayString,	-- initchat send to modem
	avSPortSttyCmd		DisplayString,
	avSPortSockTx		Integer32,	-- socket transmission interval
	avSPortSockPoll		Integer32,	-- socket polling interval
	avSPortSockIdle		Integer32,	-- socket idle timeout
	avSPortDBsize		Integer32,	-- maximum size of the data buffering file
	avSPortDBtime		INTEGER,	-- data buffering with/without timestamp
	avSPortDBmode		DisplayString,	-- data bufferinf mode circ or line
	avSPortDBsyslog		Integer32,	-- size of the data send to syslog
	avSPortDBmenu		INTEGER,	-- menu of the data buffering
	avSPortDBalarm		INTEGER,	-- use alarm generating feature
	avSPortSSHbreak		DisplayString,	-- break sequence (SSH)
	avSPortSniffSess	DisplayString,	-- sniff session type
	avSPortSniffAdm		DisplayString,	-- admin users to sniff
	avSPortSniffEsc		DisplayString,	-- escape char to sniff session
	avSPortSniffMsess	DisplayString,	-- multiple session
	avSPortTelnetMode	INTEGER,	-- telnet client mode
	avSPortSysBufSess	INTEGER,	-- syslog buffering at all times
	avSPortLFSuppress	INTEGER,	-- LF suppression in socket_server session
	avSPortAutoInput	DisplayString,  -- Input string to auto answer 
	avSPortAutoOutput	DisplayString,  -- Output string to auto answer
	avSPortPmType 		DisplayString,  -- Power Management IPDU type
	avSPortPmUsers	 	DisplayString,  -- Power Management IPDU users
	avSPortPmOutlet 	DisplayString,  -- Power Management Outlets
	avSPortPmKey 		DisplayString,  -- Power Management Hotkey
	avSPortPmNOutlets 	Integer32, 	-- Power Management IPDU number of outlets 
	avSPortBreakInterval 	Integer32 	-- Break interval (in ms) 
}

avSPortNumber	OBJECT-TYPE
	SYNTAX		InterfaceIndexOrZero	
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION	"number to identifier the serial port. Value 0 is ALL."
::= { avsportEntry 1 }

avSPortTty		OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..25))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The device name for the serial port."
::= { avsportEntry 2 }

avSPortName		OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..40))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"Alias name given to the server connected to the serial port."
::= { avsportEntry 3 }

avSPortSpeed	OBJECT-TYPE
	SYNTAX		INTEGER {
				s50bps(50), s75bps(75), s110bps(110), s134bps(134), 
				s150bps(150), s200bps(200), s300bps(300), s600bps(600), 
				s1200bps(1200), s1800bps(1800),
				s2400bps(2400), s4800bps(4800), 
				s9600bps(9600), s14400bps(14400), 
				s19200bps(19200), s28800bps(28800), 
				s38400bps(38400), s57600bps(57600), 
				s115200bps(115200), s230400bps(230400), 
				s460800bps(460800)
			}	
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The speed for serial port."
::= { avsportEntry 4 }

avSPortDataSize	OBJECT-TYPE
	SYNTAX		Integer32 (4..8)
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The data size for serial port."
::= { avsportEntry 5 }

avSPortStopBits	OBJECT-TYPE
	SYNTAX		Integer32 (1..2)
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The number of stop bits for serial port."
::= { avsportEntry 6 }

avSPortParity	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..5))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The parity for serial port."
::= { avsportEntry 7 }

avSPortFlowCtrl	OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..5))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"The flow control for serial port."
::= { avsportEntry 8 }

avSPortDTRdelay	OBJECT-TYPE
	SYNTAX		Integer32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	"This specifies how long (in miliseconds) a DTR signal
				 will be turned off before it is turned on again."
::= { avsportEntry 9 }

avSPortDCDCtrl	OBJECT-TYPE
	SYNTAX		INTEGER {
				notctrl(0),  
				control(1)	   
			}
	MAX-ACCESS	read-write
