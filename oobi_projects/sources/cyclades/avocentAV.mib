-- Avocent Vendor AutoView Family MIB OID Tree
-- Release 1.0.0 26/04/2007

AVOCENT-MIB DEFINITIONS ::= BEGIN

IMPORTS
   enterprises
      FROM SNMPv2-SMI ;

avocent		OBJECT IDENTIFIER ::= { enterprises 10418 }
autoview	OBJECT IDENTIFIER ::= { avocent 15 }
avProducts	OBJECT IDENTIFIER ::= { autoview 1 }

avCS4001	OBJECT IDENTIFIER ::= { avProducts 1 }
avCS4004	OBJECT IDENTIFIER ::= { avProducts 2 }
avCS4008	OBJECT IDENTIFIER ::= { avProducts 3 }
avCS4016	OBJECT IDENTIFIER ::= { avProducts 4 }

END

-- AutoView Family MIB management

AVOCENT-AUTOVIEW-MIB DEFINITIONS ::= BEGIN

IMPORTS
	autoview
		FROM AVOCENT-MIB
	MODULE-IDENTITY
		FROM SNMPv2-SMI;

avManagement MODULE-IDENTITY
	LAST-UPDATED	"200704260000Z"
	ORGANIZATION	"Avocent Corporation"
	CONTACT-INFO	"postal: Avocent Corporation
			3541 Gateway Boulevard
			Fremont, CA 94538, USA
			e-mail: Technical Support
			techsupport@avocent.com"
	DESCRIPTION	"This file defines the Avocent MIB extensions to 
		AutoView Family products"
	REVISION	"200704260000Z"
	DESCRIPTION	"First Draft"
::= { autoview 2 }

-- Current Avocent AutoView Family core mib table entries:
--	avSystem			OBJECT-IDENTITY  ::= { avManagement 1 }
--	avConfiguration		OBJECT-IDENTITY  ::= { avManagement 2 }
